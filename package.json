{
  "name": "@chingu-x/modules",
  "description": "Module with all of the business logic in Chingu dashboard projects",
  "version": "0.3.2",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "GPL-3.0",
  "keywords": [],
  "author": "",
  "scripts": {
    "build": "vite build && tsc",
    "lint": "eslint",
    "prepublishOnly": "pnpm build",
    "prepare": "husky install",
    "prettier": "prettier . --write",
    "format:changed": "git diff --name-only --diff-filter=d HEAD | grep -e 'src.*\\.[jt]sx\\?$' -e 'src.*\\.json$' -e 'src.*\\.css$' -e 'src.*\\.md$' | xargs prettier -u --write",
    "test": "vitest",
    "test-ci": "vitest --run"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./di-types": {
      "import": "./dist/types.js",
      "types": "./dist/types/types.d.ts"
    },
    "./resolver": {
      "import": "./dist/resolver.js",
      "types": "./dist/types/resolver.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.js",
      "types": "./dist/types/auth/index.d.ts"
    },
    "./rest-api": {
      "import": "./dist/rest-api/index.js",
      "types": "./dist/types/rest-api/index.d.ts"
    },
    "./test": {
      "import": "./dist/test/index.js",
      "types": "./dist/types/test/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/types/index.d.ts"
    }
  },
  "dependencies": {
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0"
  }
}
